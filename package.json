{
  "name": "angular-spectre",
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/GaZaTu/angular-spectre.git"
  },
  "bugs": {
    "url": "https://github.com/GaZaTu/angular-spectre/issues"
  },
  "scripts": {
    "ng": "ng",
    "ivy-ngcc": "ivy-ngcc",
    "lib:build": "ng build angular-spectre",
    "lib:build-ivy": "ng build angular-spectre && ivy-ngcc --source=dist/angular-spectre",
    "lib:watch": "ng build angular-spectre --watch",
    "lib:npm": "node ./scripts/npm",
    "start": "ng serve angular-spectre-test",
    "build": "ng build angular-spectre-test",
    "dist": "ng build angular-spectre-test --aot=true --buildOptimizer=true --extractCss=true --namedChunks=false --optimization=true --outputHashing=all --prod=true --progress=true --sourceMap=false",
    "bundle-report": "ng build angular-spectre-test --aot=true --buildOptimizer=true --extractCss=true --namedChunks=false --optimization=true --outputHashing=all --prod=true --progress=true --sourceMap=false --stats-json && webpack-bundle-analyzer dist/angular-spectre-test/stats-es2015.json",
    "analyze-bundle": "webpack-bundle-analyzer dist/angular-spectre-test/stats-es2015.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "@angular/animations": "~8.0.0-rc.0",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "~8.0.0-rc.0",
    "@angular/compiler": "~8.0.0-rc.0",
    "@angular/core": "~8.0.0-rc.0",
    "@angular/forms": "~8.0.0-rc.0",
    "@angular/platform-browser": "~8.0.0-rc.0",
    "@angular/platform-browser-dynamic": "~8.0.0-rc.0",
    "@angular/pwa": "^0.12.4",
    "@angular/router": "~8.0.0-rc.0",
    "@angular/service-worker": "~8.0.0-rc.0",
    "@auth0/angular-jwt": "^2.1.0",
    "@ngx-translate/core": "^11.0.1",
    "classnames": "^2.2.6",
    "lz-string": "^1.4.4",
    "rxjs": "~6.5.1",
    "spectre.css": "^0.5.8",
    "tslib": "^1.9.3",
    "zone.js": "~0.9.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0-rc.0",
    "@angular-devkit/build-ng-packagr": "~0.800.0-rc.0",
    "@angular/cli": "~8.0.0-rc.0",
    "@angular/compiler-cli": "~8.0.0-rc.0",
    "@angular/language-service": "~8.0.0-rc.0",
    "@types/jasmine": "~3.3.12",
    "@types/jasminewd2": "~2.0.6",
    "@types/lz-string": "^1.3.33",
    "@types/node": "~11.13.7",
    "codelyzer": "~5.0.1",
    "cross-spawn": "^6.0.5",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^5.1.0",
    "protractor": "~5.4.2",
    "ts-node": "~8.1.0",
    "tsickle": ">=0.34.3",
    "tslib": "^1.9.3",
    "tslint": "~5.16.0",
    "typescript": "~3.4.5",
    "webpack-bundle-analyzer": "^3.3.2"
  }
}
